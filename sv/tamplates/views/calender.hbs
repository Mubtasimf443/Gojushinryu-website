          <button id="prev"><</button>
          <button id="next">></button>
        </nav>
      </div>
      <div class="cl-sec">
        <div class="days">
          <div class="day-list">Sun</div>
          <div class="day-list">Mon</div>
          <div class="day-list">Tue</div>
          <div class="day-list">Wed</div>
          <div class="day-list">Thu</div>
          <div class="day-list">Fri</div>
          <div class="day-list">Sat</div>
        </div>
        <div class="dates"></div>
      </div>
      <div class="base-calender">
        <div class="cl_time_div">
          <div class="icon">
            <i class="fa-regular fa-calendar-days"></i>
          </div>
          <div class="column">
            <b>Seniur</b>
            <span>7:00PM - 9:00PM </span>
          </div>

        </div>
         <div class="cl_time_div">
